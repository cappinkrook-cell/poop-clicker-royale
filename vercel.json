{
  "version": 2,
  "name": "poop-clicker-royale-client",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "env": {
    "VITE_SERVER_URL": ""
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}